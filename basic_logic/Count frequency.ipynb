      "Counter({'e': 2, 'O': 1, 'm': 1, 'j': 1})\n"
     ]
    }
   ],
   "source": [
    "from collections import Counter\n",
    "string = 'Omjee'\n",
    "print(Counter(string))  # with use of counter"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "cb65e726-d6c6-4ae8-b9fc-b530c348a3b9",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "{'O': 1, 'm': 1, 'j': 1, 'e': 2}\n"
     ]
    }
   ],
   "source": [
    "freq = {}\n",
    "for ch in string:  # without counter \n",
    "    freq[ch] = freq.get(ch,0)+1\n",
    "\n",
    "print(freq)"
   ]
  },
